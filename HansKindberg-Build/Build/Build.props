<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<Import Project="..\..\Build\Package-Build.props" />
	<PropertyGroup>
		<HansKindbergBuildXmlTransformationContentVersion Condition="$(HansKindbergBuildXmlTransformationContentVersion) == ''">1.0.0-alpha-0</HansKindbergBuildXmlTransformationContentVersion>
		<VSToolsPath Condition="$(VSToolsPath) == ''">$(MSBuildExtensionsPath32)\Microsoft\VisualStudio\v$(VisualStudioVersion)</VSToolsPath>
	</PropertyGroup>
	<PropertyGroup>
		<HansKindbergBuildXmlTransformationContentDirectory Condition="$(HansKindbergBuildXmlTransformationContentDirectory) == ''">$(NuGetPackagesDirectory)HansKindberg-Build-XmlTransformation-Content.$(HansKindbergBuildXmlTransformationContentVersion)\</HansKindbergBuildXmlTransformationContentDirectory>
	</PropertyGroup>
	<PropertyGroup>
		<HansKindbergBuildXmlTransformationFilePrefixPath Condition="$(HansKindbergBuildXmlTransformationFilePrefixPath) == ''">$(HansKindbergBuildXmlTransformationContentDirectory)build\HansKindberg-Build-XmlTransformation</HansKindbergBuildXmlTransformationFilePrefixPath>
	</PropertyGroup>
	<PropertyGroup>
		<HansKindbergBuildXmlTransformationPropertiesFile Condition="$(HansKindbergBuildXmlTransformationPropertiesFile) == ''">$(HansKindbergBuildXmlTransformationFilePrefixPath).props</HansKindbergBuildXmlTransformationPropertiesFile>
		<HansKindbergBuildXmlTransformationTargetsFile Condition="$(HansKindbergBuildXmlTransformationTargetsFile) == ''">$(HansKindbergBuildXmlTransformationFilePrefixPath).targets</HansKindbergBuildXmlTransformationTargetsFile>
	</PropertyGroup>
	<UsingTask AssemblyFile="$(VSToolsPath)\Web\Microsoft.Web.Publishing.Tasks.dll" TaskName="Microsoft.Web.Publishing.Tasks.TransformXml" />
</Project>